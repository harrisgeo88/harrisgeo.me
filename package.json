    "type-check": "tsc --noEmit",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "new:blog": "plop --require esm",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watchAll --colors"
  },
  "repository": {
    "type": "git",
