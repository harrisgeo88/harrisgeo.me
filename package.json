{
  "name": "harris-geo-blog",
  "private": false,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Harris Geo <charilaoss@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:ts": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
