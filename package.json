    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-source-prismic": "3.0.0-imgix.5",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.1.17",
    "html-to-react": "^1.4.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
