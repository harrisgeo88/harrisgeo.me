    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.21.20",
    "gatsby-cli": "^2.12.13",
    "gatsby-image": "^2.0.33",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.24",
